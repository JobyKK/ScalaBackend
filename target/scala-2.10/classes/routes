# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

#Employee
POST    /employee/getAll      		controllers.EmployeeController.getEmployees
POST    /employee/getById      		controllers.EmployeeController.getById
POST    /employee/deleteById      	controllers.EmployeeController.deleteById
POST    /employee/newEmployee      	controllers.EmployeeController.saveEmployee
POST    /employee/updateEmployee    controllers.EmployeeController.updateEmployee

#Resume
POST    /resumes                    controllers.ResumeController.getResumes
POST    /employee/newResume         controllers.ResumeController.saveResume
POST    /employee/updateResume      controllers.ResumeController.updateResume
POST    /getResume                  controllers.ResumeController.getResume
POST    /deleteByIdResume           controllers.ResumeController.deleteById
